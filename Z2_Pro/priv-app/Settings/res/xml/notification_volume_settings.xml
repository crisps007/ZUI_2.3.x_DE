    <PreferenceCategory android:title="@string/ringtone_volume_title" android:key="ringtone" />
    <com.android.settings.notification.VolumeSeekBarPreference android:key="ringtone_volume" />
    <PreferenceCategory android:state_expanded="true" android:title="@string/call_volume_title" android:key="call" />
